#!system-python
import os
import subprocess

p = os.path.expanduser("~/ghq/github.com/74th/mycheatsheets/")

print(f"cd {p} && git pull")
subprocess.run(
    ["git", "pull"],
    check=True,
    capture_output=True,
    cwd=p,
)
print(f"c {p}")
subprocess.run(
    ["c", f"{p}/sheets"],
    check=True,
    capture_output=True,
)
