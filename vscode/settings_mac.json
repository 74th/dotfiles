{
    // font
    "editor.fontFamily": "SourceCodePro-Light,SourceHanCodeJP-Light"
    //"editor.fontFamily": "Source Code Pro Light,SourceHanCodeJP-Light, Source Han Code JP Light,Source Han Code JP L"
    ,"editor.fontSize": 13
    // wrap
    ,"editor.wrappingColumn": 0
    ,"editor.wrappingIndent": "indent"
    // tab
    ,"editor.tabSize": 4
    ,"editor.insertSpaces": false
    ,"editor.renderWhitespace": true
    ,"editor.cursorStyle": "line"
    // auto format"
    ,"editor.formatOnType": true
    ,"editor.lineNumbers": true
    ,"editor.selectionHighlight": false

    // golang
    ,"go.lintOnSave": true
    ,"go.formatTool": "goreturns"
    ,"go.formatOnSave": true
    ,"go.buildOnSave": true
    ,"go.vetOnSave": true

    // git
    ,"git.path": "/usr/local/bin/git"

    // vimStyle
    
    // mykeyscroll
    ,"scrollkey.line1": 1
    ,"scrollkey.line2": 30
    ,"scrollkey.line3": 60
}
