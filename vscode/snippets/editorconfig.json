{
  "default": {
    "prefix": "default",
    "body": [
      "root = true",
      "",
      "[*]",
      "end_of_line = lf",
      "insert_final_newline = true",
      "",
      "[*.{yaml,json}]",
      "indent_style = space",
      "indent_size = 2"
    ],
    "description": "*,*.yaml"
  },
  "python": {
    "prefix": "python",
    "body": ["[*.py]", "indent_style = space", "indent_size = 4"],
    "description": ".py"
  },
  "typescript": {
    "prefix": "typescript",
    "body": ["[*.{js,ts,tsc}]", "indent_style = space", "indent_size = 4"],
    "description": ".ts, .tsx, .js"
  }
}
