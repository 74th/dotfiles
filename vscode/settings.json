{
    "workbench.colorCustomizations": {
    },
    "terminal.external.osxExec": "Terminal.app",
    // font
    "editor.fontFamily": "Liga InputMono,Hasklig,Hasklig Light,SourceCodePro,Source Code Pro,SourceHanCodeJP,Source Han Code JP,TakaoGothic",
    "editor.fontLigatures": true,
    "editor.fontWeight": "400",
    // For Linux
    //"editor.fontFamily": "Source Code Pro Medium,Source Han Code JP Medium,TakaoGothic",
    "editor.fontSize": 14,
    // java
    "java.referencesCodeLens.enabled": false,
    // svg
    "svgviewer.transparencygrid": false,
    // wrap
    "editor.wordWrap": "on",
    "editor.wrappingIndent": "indent",
    "editor.glyphMargin": true,
    "editor.formatOnPaste": true,
    "python.linting.lintOnSave": true,
    // tab等の表示
    "editor.tabSize": 4,
    "editor.insertSpaces": false,
    "editor.renderWhitespace": "all",
    "editor.renderIndentGuides": true,
    "editor.renderControlCharacters": true,
    // カーソル
    "editor.cursorStyle": "line",
    "editor.cursorBlinking": "smooth",
    "editor.renderLineHighlight": "all",
    // 表示周り
    "window.zoomLevel": 0,
    // git
    "git.confirmSync": false, // Syncボタンで尋ねない
    // auto format
    "editor.formatOnType": true,
    "editor.lineNumbers": "on",
    "editor.selectionHighlight": false,
    "editor.autoClosingBrackets": false,
    // file
    "files.eol": "\n",
    "files.trimTrailingWhitespace": true,
    // golang
    "go.lintOnSave": "package",
    "go.formatTool": "goreturns",
    "go.buildOnSave": "package",
    "go.vetOnSave": "package",
    "go.testTimeout": "180s",
    "go.toolsGopath": "~/go",
    "go.coverOnTestPackage": true,
    // typescript
    "[typescript]":{
        "editor.codeLens": false,
    },
    // vimStyle
    "vimStyle.defaultMode": "normal",
    "vimStyle.enabled": true,
    "vimStyle.imapEsc": "jj",

    "[python]": {
        "editor.detectIndentation": false,
        "editor.insertSpaces": true,
        "editor.tabSize": 4,
        "editor.formatOnSave": false,
        "python.unitTest.pyTestEnabled": true,
        "python.jediEnabled": false,
        // Lintは有効にする
        "python.linting.enabled": true,
        "python.formatting.provider": "yapf",
        // 標準はpython3.7に設定する
        "python.pythonPath": "/usr/local/bin/python3.7",
        "python.formatting.yapfArgs": [
            "--style={COLUMN_LIMIT: 120}"
        ],
        "python.linting.mypyEnabled": true,
    },
    // terminal -------------------
    "terminal.integrated.shell.osx": "/usr/local/bin/bash",
    "terminal.integrated.shell.linux": "bash",
    // minimap
    "editor.minimap.enabled": false,
    // plantuml
    // "plantuml.server": "http://nagisa:18080/",
    // "plantuml.render": "PlantUMLServer",
    // cordova

    // mykeyscroll
    "scrollkey.line1": 30,
    "workbench.colorTheme": "monokai-chacoral(HC)",
    "workbench.startupEditor": "newUntitledFile",
    "[cuda]": {
		"editor.formatOnSave": true
    },
    "cSpell.language": "en,en-US,en-GB",
    "cSpell.enabledLanguageIds": [
        "asciidoc",
        "c",
        "cpp",
        "csharp",
        "css",
        "go",
        "handlebars",
        "html",
        "jade",
        "javascript",
        "javascriptreact",
        "json",
        "jsonc",
        "latex",
        "less",
        "markdown",
        "php",
        "plaintext",
        "pub",
        "python",
        "restructuredtext",
        "rust",
        "scss",
        "text",
        "typescript",
        "typescriptreact",
        "yaml",
        "yml"
    ],
    "cSpell.ignorePaths": [
        "**/package-lock.json",
        "**/node_modules/**",
        "**/vscode-extension/**",
        "**/.git/**",
        ".vscode",
        "typings"
    ],
    "cSpell.dictionaryDefinitions": [
        {
            "name": "my_work",
            "path": "/Users/nnyn/mycheatsheets/CodeSpellChecker/work.txt"
        },
        {
            "name": "my_it",
            "path": "/Users/nnyn/mycheatsheets/CodeSpellChecker/it.txt"
        },
        {
            "name": "my_abbreviation",
            "path": "/Users/nnyn/mycheatsheets/CodeSpellChecker/abbreviation.txt"
        },
        {
            "name": "my_word",
            "path": "/Users/nnyn/mycheatsheets/CodeSpellChecker/me.txt"
        }
    ],
    "cSpell.dictionaries": [
        "my_work",
        "my_it",
        "my_abbreviation",
        "my_word"
    ],
    "files.associations": {
        "*.jinja": "yaml",
        "*.md": "markdown",
    },
    "git.autofetch": true,
    "search.exclude": {
        "**/.mypy_cache": true,
        "**/.vscode": true,
        "**/*.pyc": true,
        "**/tmp": true,
    },
    "diffEditor.ignoreTrimWhitespace": false,
    "cfnLint.path": "/usr/local/bin/cfn-lint",
    "breadcrumbs.enabled": true,
    "files.exclude": {
        "**/__pycache__": true,
        "**/.mypy_cache": true,
        "**/*.swp": true,
        "**/.classpath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.factorypath": true
    },
    "scm.alwaysShowProviders": true,
    "scm.alwaysShowActions": true,
    "workbench.iconTheme": "Monokai Pro Icons",
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "extensions.autoCheckUpdates": false,
    "extensions.autoUpdate": false,
    "python.jediEnabled": false,
    "cloudcode.gke": {
        "projectIds": [
            "dena-auto-taxifms-datadev-gcp"
        ]
    },
}
