[
  {
    "key": "ctrl+[",
    "command": "vim.Esc",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+w",
    "command": "deleteWordLeft",
    "when": "vim.inInsertMode"
  },
  {
    "key": "ctrl+w",
    "command": "-workbench.action.closeWindow",
    "when": "vim.inInsertMode"
  },
  {
    "key": "ctrl+d",
    "command": "editorScroll",
    "args": {
      "to": "down",
      "by": "halfPage",
      "revealCursor": true,
      "value": 1
    }
  },
  {
    "key": "ctrl+u",
    "command": "editorScroll",
    "args": {
      "to": "up",
      "by": "halfPage",
      "revealCursor": true,
      "value": 0
    }
  },
  {
    "key": "ctrl+1",
    "command": "workbench.action.showCommands"
  },
  {
    "key": "f2",
    "command": "workbench.action.quickOpen"
  },
  {
    "key": "ctrl+2",
    "command": "workbench.action.quickOpen"
  },
  {
    "key": "shift+f2",
    "command": "workbench.action.quickOpen"
  },
  {
    "key": "ctrl+shift+2",
    "command": "workbench.action.quickOpen"
  },
  {
    "key": "ctrl+3",
    "command": "editor.action.nextMatchFindAction",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+shift+3",
    "command": "editor.action.previousMatchFindAction",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+5",
    "command": "workbench.action.debug.start",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+5",
    "command": "workbench.action.debug.continue",
    "when": "!inDebugMode"
  },
  {
    "key": "f9",
    "command": "editor.action.quickFix",
    "when": "!inDebugMode && editorTextFocus"
  },
  {
    "key": "ctrl+w ctrl+w",
    "command": "workbench.action.closeActiveEditor",
    "when": "!vim.inInsertMode"
  },
  {
    "key": "meta+w",
    "command": "workbench.action.closeActiveEditor",
    "when": "isLinux"
  },
  {
    "key": "ctrl+w l",
    "command": "workbench.action.focusFirstEditorGroup",
    "when": "sideBarFocus"
  },
  {
    "key": "ctrl+w l",
    "command": "workbench.action.focusRightGroup",
    "when": "editorFocus"
  },
  {
    "key": "ctrl+w h",
    "command": "workbench.action.focusLeftGroup",
    "when": "editorFocus && activeEditorGroupIndex != 1"
  },
  {
    "key": "ctrl+w h",
    "command": "workbench.action.focusSideBar",
    "when": "editorFocus && activeEditorGroupIndex == 1"
  },
  {
    "key": "ctrl+w j",
    "command": "workbench.action.focusPanel"
  },
  {
    "key": "ctrl+w k",
    "command": "workbench.action.focusAboveGroup"
  },
  {
    "key": "ctrl+w h",
    "command": "workbench.panel.markers.view.focus",
    "when": "panelFocus && activePanel == 'workbench.panel.output'"
  },
  {
    "key": "ctrl+w ctrl+w",
    "command": "workbench.panel.markers.view.focus",
    "when": "panelFocus && activePanel == 'workbench.panel.terminal'"
  },
  {
    "key": "cmd+w l",
    "command": "workbench.action.positionPanelRight",
    "when": "panelFocus"
  },
  {
    "key": "ctrl+p",
    "command": "showPrevParameterHint",
    "when": "editorTextFocus && parameterHintsMultipleSignatures && parameterHintsVisible"
  },
  {
    "key": "ctrl+n",
    "command": "showNextParameterHint",
    "when": "editorTextFocus && parameterHintsMultipleSignatures && parameterHintsVisible"
  },
  {
    "key": "ctrl+p",
    "command": "selectPrevSuggestion",
    "when": "editorTextFocus && suggestWidgetMultipleSuggestions && suggestWidgetVisible"
  },
  {
    "key": "ctrl+n",
    "command": "selectNextSuggestion",
    "when": "editorTextFocus && suggestWidgetMultipleSuggestions && suggestWidgetVisible"
  },
  {
    "key": "ctrl+p",
    "command": "workbench.action.quickOpenNavigatePrevious",
    "when": "inQuickOpen"
  },
  {
    "key": "ctrl+n",
    "command": "workbench.action.quickOpenNavigateNext",
    "when": "inQuickOpen"
  },
  {
    "key": "ctrl+h",
    "command": "deleteLeft",
    "when": "editorTextFocus && vim.inInsertMode"
  },
  {
    "key": "f10",
    "command": "go.test.cursor",
    "when": "editorTextFocus && editorLangId == 'go' && !inDebugMode"
  },
  {
    "key": "f10",
    "command": "python.runCurrentTestFile",
    "when": "editorTextFocus && editorLangId == 'python' && !inDebugMode"
  },
  {
    "key": "ctrl+0",
    "command": "go.test.cursor",
    "when": "editorTextFocus && editorLangId == 'go' && !inDebugMode"
  },
  {
    "key": "cmd+r",
    "command": "editor.action.rename",
    "when": "editorHasRenameProvider && editorTextFocus && !editorReadonly"
  },
  {
    "key": "alt+r",
    "command": "editor.action.rename",
    "when": "editorHasRenameProvider && editorTextFocus && !editorReadonly"
  },
  {
    "key": "cmd+r",
    "command": "renameFile",
    "when": "filesExplorerFocus"
  },
  {
    "key": "ctrl+r",
    "command": "renameFile",
    "when": "filesExplorerFocus"
  },
  {
    "key": "alt+r",
    "command": "renameFile",
    "when": "filesExplorerFocus"
  },
  {
    "key": "alt+q",
    "command": "editor.action.quickFix"
  },
  {
    "key": "cmd+q",
    "command": "editor.action.quickFix"
  },
  {
    "key": "alt+s",
    "command": "saveAll",
    "when": "isLinux"
  },
  {
    "key": "ctrl+shift+w",
    "command": "workbench.action.closeWindow"
  },
  {
    "key": "cmd+q",
    "command": "workbench.action.closeWindow"
  },
  {
    "key": "cmd+1",
    "command": "workbench.action.terminal.focusAtIndex1",
    "when": "terminalFocus"
  },
  {
    "key": "ctrl+1",
    "command": "workbench.action.terminal.focusAtIndex1",
    "when": "terminalFocus"
  },
  {
    "key": "cmd+2",
    "command": "workbench.action.terminal.focusAtIndex2",
    "when": "terminalFocus"
  },
  {
    "key": "ctrl+2",
    "command": "workbench.action.terminal.focusAtIndex2",
    "when": "terminalFocus"
  },
  {
    "key": "cmd+3",
    "command": "workbench.action.terminal.focusAtIndex3",
    "when": "terminalFocus"
  },
  {
    "key": "ctrl+3",
    "command": "workbench.action.terminal.focusAtIndex3",
    "when": "terminalFocus"
  },
  {
    "key": "cmd+4",
    "command": "workbench.action.terminal.focusAtIndex4",
    "when": "terminalFocus"
  },
  {
    "key": "ctrl+4",
    "command": "workbench.action.terminal.focusAtIndex4",
    "when": "terminalFocus"
  },
  {
    "key": "cmd+5",
    "command": "workbench.action.terminal.focusAtIndex5",
    "when": "terminalFocus"
  },
  {
    "key": "ctrl+5",
    "command": "workbench.action.terminal.focusAtIndex5",
    "when": "terminalFocus"
  },
  {
    "key": "cmd+t",
    "command": "workbench.action.quickOpen",
    "args": ">Shell Launcher: Launch",
    "when": "terminalFocus"
  },
  {
    "key": "ctrl+t",
    "command": "workbench.action.quickOpen",
    "args": ">Shell Launcher: Launch",
    "when": "terminalFocus"
  },
  {
    "key": "shift+enter",
    "command": "-python.execSelectionInTerminal",
    "when": "editorTextFocus && !findInputFocussed && !python.datascience.ownsSelection && !replaceInputFocussed && editorLangId == 'python'"
  },
  {
    "key": "ctrl+]",
    "command": "workbench.files.action.focusFilesExplorer"
  },
  {
    "key": "shift+f12",
    "command": "workbench.action.navigateBack"
  },
  {
    "key": "ctrl+alt+-",
    "command": "-workbench.action.navigateBack"
  },
  {
    "key": "ctrl+left",
    "command": "workbench.action.previousEditor"
  },
  {
    "key": "ctrl+right",
    "command": "workbench.action.nextEditor"
  },
  {
    "key": "alt+`",
    "command": "workbench.action.terminal.toggleTerminal"
  },
  {
    "key": "cmd+`",
    "command": "workbench.action.terminal.toggleTerminal"
  },
  {
    "key": "ctrl+f",
    "command": "-vscode-neovim.ctrl-f",
    "when": "editorTextFocus && neovim.ctrlKeysNormal && neovim.init && neovim.mode != 'insert'"
  },
  {
    "key": "ctrl+w",
    "command": "-vscode-neovim.send",
    "when": "editorTextFocus && neovim.ctrlKeysNormal && neovim.init && neovim.mode != 'insert'"
  },
  {
    "key": "ctrl+shift+v",
    "command": "editor.action.clipboardPasteAction"
  },
  {
    "key": "ctrl+v",
    "command": "vscode-neovim.send",
    "args": "<C-v>",
    "when": "editorTextFocus && neovim.init && neovim.mode != 'insert'"
  },
  {
    "key": "f5",
    "command": "workbench.action.debug.selectandstart",
    "when": "!inDebugMode"
  },
  {
    "key": "cmd+f5",
    "command": "workbench.action.debug.selectandstart"
  },
  {
    "key": "enter",
    "command": "-renameFile",
    "when": "explorerViewletVisible && filesExplorerFocus && !explorerResourceIsRoot && !explorerResourceReadonly && !inputFocus"
  },
  {
    "key": "f9",
    "command": "-editor.debug.action.toggleBreakpoint",
    "when": "editorTextFocus"
  }
]